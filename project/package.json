  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.3.1",
    "@types/validator": "^13.15.3",
    "module-alias": "^2.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
